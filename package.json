{
  "name": "goodstore",
  "version": "0.0.1",
  "description": "A Datastore module that abstracts the loading of data from the usage of data",
  "main": "./dist/goodstore.es5.umd.min.js",
  "module": "./dist/lib/index.js",
  "typings": "./dist/goodstore.d.ts",
  "unpkg": "dist/goodstore.es5.iife.min.js",
  "files": [
    "dist/goodstore.es5.iife.min.js",
    "dist/goodstore.es5.umd.min.js",
    "dist/goodstore.es5.es2015.min.js",
    "dist/goodstore.es6.min.js",
    "dist/goodstore.es6.umd.min.js",
    "dist/goodstore.d.ts",
    "dist/goodstore.iife.d.ts",
    "dist/lib/"
  ],
  "scripts": {
    "install_tools": "npm install && typings install",
    "clean": "rimraf ./dist",
    "clean-test": "rimraf ./dist/test",
    "tsc": "tsc",
    "rollup": "node rollup.js",
    "build": "npm run clean && tsc -p tsconfig.app.json && gulp build-es5 && tsc -p tsconfig.es6.json && gulp build-es6 && node dts-bundle.js && gulp copyDTS",
    "test": "npm run clean && tsc -p tsconfig-test.json && npm run run_test",
    "run_test": "mocha ./dist/test/*Test.js",
    "testcoverage": "npm run clean && tsc -p tsconfig-test.json && npm run coveralls",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./dist/test/*.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./dist/test/*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "bundle_d_ts": "node dts-bundle.js"
  },
  "dependencies": {
    "es6-promise": "^4.2.2",
    "goodcore": "^0.16.9",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "@types/rollup": "^0.41.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "dts-bundle": "^0.7.3",
    "gulp-debug": "^3.2.0",
    "gulp-sequence": "^1.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.5.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "mocha": "^4.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-commonjs": "^8.2.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "source-map-support": "^0.5.1",
    "tslib": "^1.8.1",
    "typescript": "^2.6.2",
    "typings": "^2.1.0",
    "uglify-es": "^3.3.7",
    "uglify-js": "^3.3.7"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Typescript",
    "Javascript"
  ],
  "author": "Joel Brage",
  "license": "MIT"
}
